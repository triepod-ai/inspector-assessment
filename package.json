{
  "name": "@bryan-thompson/inspector-assessment",
  "version": "1.28.0",
  "description": "Enhanced MCP Inspector with comprehensive assessment capabilities for server validation",
  "license": "MIT",
  "author": "Bryan Thompson <bryan@triepod.ai>",
  "contributors": [
    "Anthropic, PBC (original MCP Inspector)"
  ],
  "homepage": "https://github.com/triepod-ai/inspector-assessment",
  "bugs": "https://github.com/triepod-ai/inspector-assessment/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/triepod-ai/inspector-assessment.git"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "inspector",
    "assessment",
    "security",
    "testing",
    "validation"
  ],
  "type": "module",
  "main": "./client/lib/services/assessment/AssessmentOrchestrator.js",
  "types": "./client/lib/services/assessment/AssessmentOrchestrator.d.ts",
  "exports": {
    ".": {
      "types": "./client/lib/services/assessment/AssessmentOrchestrator.d.ts",
      "default": "./client/lib/services/assessment/AssessmentOrchestrator.js"
    },
    "./types": {
      "types": "./client/lib/lib/assessment/index.d.ts",
      "default": "./client/lib/lib/assessment/index.js"
    },
    "./config": {
      "types": "./client/lib/lib/assessment/configTypes.d.ts",
      "default": "./client/lib/lib/assessment/configTypes.js"
    },
    "./results": {
      "types": "./client/lib/lib/assessment/resultTypes.d.ts",
      "default": "./client/lib/lib/assessment/resultTypes.js"
    },
    "./progress": {
      "types": "./client/lib/lib/assessment/progressTypes.d.ts",
      "default": "./client/lib/lib/assessment/progressTypes.js"
    },
    "./annotations": {
      "types": "./client/lib/services/assessment/modules/annotations/index.d.ts",
      "default": "./client/lib/services/assessment/modules/annotations/index.js"
    },
    "./performance": {
      "types": "./client/lib/services/assessment/config/performanceConfig.d.ts",
      "default": "./client/lib/services/assessment/config/performanceConfig.js"
    },
    "./modules": {
      "types": "./client/lib/services/assessment/modules/index.d.ts",
      "default": "./client/lib/services/assessment/modules/index.js"
    },
    "./security": {
      "types": "./client/lib/services/assessment/modules/securityTests/index.d.ts",
      "default": "./client/lib/services/assessment/modules/securityTests/index.js"
    }
  },
  "bin": {
    "mcp-inspector-assess": "cli/build/cli.js",
    "mcp-assess-full": "cli/build/assess-full.js",
    "mcp-assess-security": "cli/build/assess-security.js"
  },
  "files": [
    "client/bin",
    "client/dist",
    "client/lib",
    "client/package.json",
    "server/build",
    "server/package.json",
    "cli/build",
    "cli/package.json"
  ],
  "workspaces": [
    "client",
    "server",
    "cli"
  ],
  "scripts": {
    "build": "npm run build-server && npm run build-client && npm run build-cli",
    "build-server": "cd server && npm run build",
    "build-client": "cd client && npm run build",
    "build-cli": "cd cli && npm run build",
    "clean": "rimraf ./node_modules ./client/node_modules ./cli/node_modules ./build ./client/dist ./server/build ./cli/build ./package-lock.json && npm install",
    "dev": "node client/bin/start.js --dev",
    "dev:windows": "node client/bin/start.js --dev",
    "kill": "node scripts/kill-server.js",
    "restart": "npm run kill && npm run dev",
    "assess": "tsx --tsconfig client/tsconfig.app.json scripts/run-security-assessment.ts",
    "assess:full": "test -f cli/build/assess-full.js || npm run build-cli --silent && node cli/build/assess-full.js",
    "assess:full:legacy": "tsx --tsconfig client/tsconfig.app.json scripts/run-full-assessment.ts",
    "assess:dvmcp": "bash scripts/assess-dvmcp-all.sh",
    "dev:sdk": "npm run link:sdk && concurrently \"npm run dev\" \"cd sdk && npm run build:esm:w\"",
    "link:sdk": "(test -d sdk || ln -sf ${MCP_SDK:-$PWD/../typescript-sdk} sdk) && (cd sdk && npm link && (test -d node_modules || npm i)) && npm link @modelcontextprotocol/sdk",
    "unlink:sdk": "(cd sdk && npm unlink -g) && rm sdk && npm unlink @modelcontextprotocol/sdk",
    "sync:upstream": "bash scripts/sync-upstream.sh",
    "start": "node client/bin/start.js",
    "start-server": "cd server && npm run start",
    "start-client": "cd client && npm run preview",
    "test": "npm run prettier-check && cd client && npm test",
    "test:scripts": "npx jest --config scripts/jest.config.cjs",
    "test-cli": "cd cli && npm run test",
    "test:e2e": "MCP_AUTO_OPEN_ENABLED=false npm run test:e2e --workspace=client",
    "test:mcp": "ts-node mcp-test-harness.ts --url http://localhost:3000 --token $MCP_TOKEN --badToken $MCP_BAD_TOKEN --protocol 2025-06-18 --fuzz 500",
    "validate:testbed": "npm run build-cli && node cli/build/validate-testbed.js",
    "validate:publish": "node scripts/validate-publish.js",
    "validate:tarball": "node scripts/validate-tarball.js",
    "prettier-fix": "prettier --write .",
    "prettier-check": "prettier --check .",
    "lint": "prettier --check . && cd client && npm run lint",
    "prepare": "husky && npm run build",
    "prepublishOnly": "node scripts/validate-publish.js",
    "publish-all": "npm publish --workspaces --access public && npm publish --access public",
    "update-version": "node scripts/update-version.js",
    "check-version": "node scripts/check-version-consistency.js",
    "version": "node scripts/sync-workspace-versions.js && git add ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "commander": "^14.0.2",
    "node-fetch": "^3.3.2",
    "open": "^10.2.0",
    "p-limit": "^7.2.0",
    "shell-quote": "^1.8.3",
    "spawn-rx": "^5.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.17.0",
    "@types/shell-quote": "^1.7.5",
    "ajv": "^8.17.1",
    "husky": "^9.1.7",
    "jest-fixed-jsdom": "^0.0.9",
    "lint-staged": "^16.1.5",
    "playwright": "^1.56.1",
    "prettier": "^3.7.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.4.2"
  },
  "overrides": {
    "get-intrinsic": "1.3.0"
  },
  "engines": {
    "node": ">=22.7.5"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
